<!--pages/authentication/authentication.wxml-->
<form bindsubmit="formSubmit" bindreset="formReset">
  <view class="rui-input">
    <input name="idCard" placeholder='请填写的身份证号'></input>
  </view>
  <view class="rui-input">
    <input name="realName" placeholder='请填写您的真实姓名'></input>
  </view>
  <view class="rui-btn-box">
    <button formType="submit">提交</button>
  </view>
</form>
